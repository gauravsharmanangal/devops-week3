#ext=`date '+%A' | tr '[:upper:]' '[:lower:]'`
#datenow=$(date --date "today " +%d%m%Y)
datenow=$(date --date "1 day ago" +%d%m%Y)
echo "$datenow"





                                                                echo $file
#done
                                                                { echo "open $ip
                                                                user $username $passwd
                                                                cd $remoteloc
                                                                ls  F_*$datenow.txt out.txt
                                                                close"
                                                                } | ftp -i -n -v 2>&1
                                                                list=`cat out.txt | tr -s ' ' | cut -d' ' -f9`
                                                                echo "Got list downloading the files"
                                                                for file in $list
                                                                 do

                                                                        { echo "open $ip
                                                                        user $username $passwd
                                                                        cd $remoteloc
                                                                        get $file
                                                                        close"
                                                                        } | ftp -i -n -v 2>&1
                                                                        OUT=$?
                                                                        if [ $OUT -eq 0 ];
                                                                                then
                                                                                echo "ftp OK"
                                                                        else
                                                                         echo "ftp Error: "$OUT
                                                                                { echo "open $ip
                                                                                user $username $passwd
                                                                                cd $remoteloc
                                                                                get $file

                                                                                close"
                                                                                } | ftp -i -n -v 2>&1

                                                                        fi
                                                                done
                                                        #ls SA_SPAMEAS*.$ext >newout.txt

                                                for file in `ls F*$datenow.txt`
                                                 do
                                                        #newfile=$file'_IN1'
                                                        #newfile='cmp_'$file

                                                        #mv $file $newfile
                                                        mv $file $reportpath
                                                done


