#!/bin/bash
current_time=$(date "+%Y-%m-%d %H:%M:%S")
a=$(ps -aef | grep vsftpd | grep -v grep | wc -l)
if [ $a -eq 0 ]; then
    echo "$current_time FTP is STOPPED!!!!!!"
    echo "$current_time Starting FTP"
    #/etc/init.d/vsftpd restart
     systemctl start vsftpd.service
else
    echo "$current_time FTP is RUNNING"
    echo "$current_time --------------"
fi



*/30 * * * * sh /u/ainet/scripts/ftpcheck.sh >> /Custom_Log_Bkup/logs/ftpcheck.log 2>&1



